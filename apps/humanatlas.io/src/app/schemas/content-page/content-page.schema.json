{
  "$ref": "#/definitions/ContentPage",
  "definitions": {
    "AnyContentTemplate": {
      "anyOf": [
        {
          "$ref": "#/definitions/DataViewer"
        },
        {
          "$ref": "#/definitions/Markdown"
        },
        {
          "$ref": "#/definitions/PageSection"
        },
        {
          "$ref": "#/definitions/VersionedDataTable"
        },
        {
          "$ref": "#/definitions/PageTable"
        }
      ]
    },
    "Classes": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        {
          "type": "object",
          "additionalProperties": {}
        }
      ]
    },
    "ContentPageData": {
      "type": "object",
      "properties": {
        "$schema": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "subtitle": {
          "type": "string"
        },
        "action": {
          "type": "object",
          "properties": {
            "label": {
              "type": "string"
            },
            "url": {
              "type": "string"
            }
          },
          "required": ["label", "url"],
          "additionalProperties": false
        },
        "content": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AnyContentTemplate"
          }
        }
      },
      "required": ["$schema", "title", "subtitle", "content"],
      "additionalProperties": false
    },
    "DataViewer": {
      "type": "object",
      "properties": {
        "component": {
          "type": "string",
          "const": "DataViewer"
        },
        "classes": {
          "$ref": "#/definitions/Classes"
        },
        "styles": {
          "$ref": "#/definitions/Styles"
        },
        "variant": {
          "type": "string"
        },
        "githubIconsUrl": {
          "type": "string"
        },
        "releaseVersionData": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "version": {
                "type": "string"
              },
              "label": {
                "type": "string"
              },
              "date": {
                "type": "string"
              },
              "crosswalkUrl": {
                "type": "string",
                "format": "uri"
              },
              "extractionsSitesUrl": {
                "type": "string",
                "format": "uri"
              },
              "referenceOrgansUrl": {
                "type": "string",
                "format": "uri"
              },
              "organData": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "type": "string"
                    },
                    "icon": {
                      "type": "string"
                    },
                    "cards": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "label": {
                            "type": "string"
                          },
                          "alt": {
                            "type": "string"
                          },
                          "fileUrl": {
                            "type": "string",
                            "format": "uri"
                          },
                          "fullscreenUrl": {
                            "type": "string",
                            "format": "uri"
                          },
                          "sourceDataUrl": {
                            "type": "string",
                            "format": "uri"
                          },
                          "crosswalkUrl": {
                            "type": "string",
                            "format": "uri"
                          },
                          "files": {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "properties": {
                                "label": {
                                  "type": "string"
                                },
                                "url": {
                                  "type": "string",
                                  "format": "uri"
                                }
                              },
                              "required": ["label", "url"],
                              "additionalProperties": false
                            }
                          }
                        },
                        "required": ["label", "fileUrl", "sourceDataUrl", "files"],
                        "additionalProperties": false
                      }
                    }
                  },
                  "required": ["label", "icon", "cards"],
                  "additionalProperties": false
                }
              }
            },
            "required": ["version", "label", "date", "organData"],
            "additionalProperties": false
          }
        }
      },
      "required": ["component", "variant", "githubIconsUrl", "releaseVersionData"],
      "additionalProperties": false
    },
    "Markdown": {
      "type": "object",
      "properties": {
        "component": {
          "type": "string",
          "const": "Markdown"
        },
        "classes": {
          "$ref": "#/definitions/DataViewer/properties/classes"
        },
        "styles": {
          "$ref": "#/definitions/DataViewer/properties/styles"
        },
        "data": {
          "type": "string"
        },
        "src": {
          "type": "string"
        }
      },
      "required": ["component"],
      "additionalProperties": false
    },
    "PageSection": {
      "type": "object",
      "properties": {
        "component": {
          "type": "string",
          "const": "PageSection"
        },
        "classes": {
          "$ref": "#/definitions/DataViewer/properties/classes"
        },
        "styles": {
          "$ref": "#/definitions/DataViewer/properties/styles"
        },
        "tagline": {
          "type": "string"
        },
        "level": {
          "type": "integer",
          "minimum": 1,
          "maximum": 6
        },
        "anchor": {
          "type": "string"
        },
        "content": {
          "anyOf": [
            {
              "$ref": "#/definitions/AnyContentTemplate"
            },
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AnyContentTemplate"
              }
            }
          ]
        }
      },
      "required": ["component", "tagline", "content"],
      "additionalProperties": false
    },
    "PageTable": {
      "type": "object",
      "properties": {
        "component": {
          "type": "string",
          "const": "PageTable"
        },
        "classes": {
          "$ref": "#/definitions/DataViewer/properties/classes"
        },
        "styles": {
          "$ref": "#/definitions/DataViewer/properties/styles"
        },
        "columns": {
          "anyOf": [
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            }
          ]
        },
        "rows": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "number"
                },
                {
                  "type": "object",
                  "properties": {
                    "label": {},
                    "url": {
                      "type": "string"
                    }
                  },
                  "required": ["url"],
                  "additionalProperties": false
                },
                {
                  "type": "object",
                  "properties": {
                    "markdown": {
                      "type": "string"
                    }
                  },
                  "required": ["markdown"],
                  "additionalProperties": false
                }
              ]
            }
          }
        },
        "style": {
          "type": "string",
          "enum": ["alternating", "divider", "basic"],
          "default": "alternating"
        },
        "enableSort": {
          "type": "boolean",
          "default": false
        },
        "verticalDividers": {
          "type": "boolean",
          "default": false
        }
      },
      "required": ["component", "columns", "rows"],
      "additionalProperties": false
    },
    "Styles": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "object",
          "additionalProperties": {}
        }
      ]
    },
    "VersionedDataTable": {
      "type": "object",
      "properties": {
        "component": {
          "type": "string",
          "const": "VersionedDataTable"
        },
        "classes": {
          "$ref": "#/definitions/DataViewer/properties/classes"
        },
        "styles": {
          "$ref": "#/definitions/DataViewer/properties/styles"
        },
        "label": {
          "type": "string"
        },
        "selection": {
          "type": "number"
        },
        "items": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "columns": {
                "$ref": "#/definitions/PageTable/properties/columns"
              },
              "rows": {
                "$ref": "#/definitions/PageTable/properties/rows"
              },
              "style": {
                "$ref": "#/definitions/PageTable/properties/style"
              },
              "enableSort": {
                "$ref": "#/definitions/PageTable/properties/enableSort"
              },
              "verticalDividers": {
                "$ref": "#/definitions/PageTable/properties/verticalDividers"
              },
              "label": {
                "type": "string"
              }
            },
            "required": ["columns", "rows", "label"],
            "additionalProperties": false
          }
        }
      },
      "required": ["component", "label", "items"],
      "additionalProperties": false
    },
    "ContentPage": {
      "$ref": "#/definitions/ContentPageData"
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}
