{
  "name": "ftu-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/ftu-ui/src",
  "prefix": "ftu",
  "targets": {
    "build": {
      "executor": "ngx-build-plus:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "singleBundle": true,
        "outputPath": "dist/apps/ftu-ui",
        "index": "apps/ftu-ui/src/index.html",
        "main": "apps/ftu-ui/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/ftu-ui/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/ftu-ui/src/favicon.ico", "apps/ftu-ui/src/assets"],
        "styles": ["apps/ftu-ui/src/styles.scss", "apps/ftu-ui/src/assets/fonts/fonts.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["node_modules/", "libs/shared/theming/src/lib/styles"]
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "baseHref": "/hra-ui/apps/ftu-ui/",
          "outputHashing": "none",
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "2mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        },
        "staging": {
          "baseHref": "/apps/ftu-ui/",
          "outputHashing": "none"
        },
        "preview": {
          "baseHref": "/apps/ftu-ui/",
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "ftu-ui:build:production"
        },
        "development": {
          "buildTarget": "ftu-ui:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "ftu-ui:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/ftu-ui/jest.config.ts"
      }
    }
  },
  "tags": ["app"],
  "implicitDependencies": ["shared-theming"]
}
