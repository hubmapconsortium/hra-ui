{
  "$ref": "#/definitions/DocsNavigation",
  "definitions": {
    "DocsMenuItems": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "#/definitions/DocsNavigationCategory"
          },
          {
            "$ref": "#/definitions/DocsNavigationItem"
          }
        ]
      }
    },
    "DocsNavigationCategory": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "const": "category"
        },
        "label": {
          "type": "string"
        },
        "icon": {
          "type": "string"
        },
        "children": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/DocsNavigationItem"
          }
        }
      },
      "required": ["label", "icon", "children"],
      "additionalProperties": false
    },
    "DocsNavigationItem": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "const": "item"
        },
        "label": {
          "type": "string"
        },
        "icon": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "format": "uri"
        }
      },
      "required": ["type", "label", "url"],
      "additionalProperties": false
    },
    "DocsNavigation": {
      "type": "object",
      "properties": {
        "$schema": {
          "type": "string"
        },
        "menuItems": {
          "$ref": "#/definitions/DocsMenuItems"
        }
      },
      "required": ["$schema", "menuItems"],
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}
